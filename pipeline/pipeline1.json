{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['Column_1']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['Column_2']"
								},
								"sink": {
									"name": "title"
								}
							},
							{
								"source": {
									"path": "$['Column_3']"
								},
								"sink": {
									"name": "state"
								}
							},
							{
								"source": {
									"path": "$['Column_4']"
								},
								"sink": {
									"name": "area_path"
								}
							},
							{
								"source": {
									"path": "$['Column_5']"
								},
								"sink": {
									"name": "type"
								}
							},
							{
								"source": {
									"path": "$['Column_6']"
								},
								"sink": {
									"name": "assigned_to"
								}
							},
							{
								"source": {
									"path": "$['Column_7']"
								},
								"sink": {
									"name": "created_date"
								}
							},
							{
								"source": {
									"path": "$['Column_8']"
								},
								"sink": {
									"name": "closed_date"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_zfr",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_zfr",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}